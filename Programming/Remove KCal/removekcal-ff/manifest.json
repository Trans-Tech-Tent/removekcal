{

    "manifest_version": 2,
    "name": "Remove KCal",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  

  
    "content_scripts": [
      {
        "matches": ["*://deliveroo.co.uk/*", "*://*.dominos.co.uk/*", "*://*.just-eat.co.uk/*", "*://*.papajohns.co.uk/*"],
        "js": ["removekcal.js"]
      }
    ]
  
  }
  